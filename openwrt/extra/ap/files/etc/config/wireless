config wifi-device  radio0
        option type     mac80211
        option channel  1
        option hwmode   11g
        option path     'bcma0:1'
        option htmode   HT20
        # REMOVE THIS LINE TO ENABLE WIFI:
        option disabled 0

config wifi-iface
        option device   radio0
        option network  stas
        option mode     ap
        option ssid     EMILIE
        option encryption none
